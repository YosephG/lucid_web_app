{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": false,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "skipLibCheck": true
  },
  "types": [
    "arcgis-js-api",
    "node",
    "react",
    "react-dom",
    "webpack-env"
  ],
  "typeRoots": [
    "node_modules/@types"
  ],
  "include": [
    "src/*"
  ]
}
